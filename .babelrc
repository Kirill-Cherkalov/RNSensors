{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "navigation/src/components",
          "constants": "navigation/src/constants"
        },
        "cwd": "babelrc"
      }
    ]
  ]
}
